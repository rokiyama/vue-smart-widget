{
  "name": "vue-smart-widget",
  "version": "0.4.6",
  "description": "Smart widget component for Vue2.5+.",
  "author": "xiaoluoboding <xiaoluoboding@gmail.com>",
  "main": "lib/vue-smart-widget.umd.min.js",
  "unpkg": "lib/vue-smart-widget.umd.min.js",
  "keywords": [
    "widget",
    "card",
    "panel",
    "vue",
    "draggable",
    "resizable"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs",
    "build:lib": "vue-cli-service build --mode lib --target lib --dest lib src/index.js",
    "report": "vue-cli-service build --mode lib --target lib --dest lib --report src/index.js",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "deploy": "sh ./scripts/deploy.sh"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "dependencies": {
    "screenfull": "^4.2.1",
    "vue-grid-layout": "git+https://github.com/rokiyama/vue-grid-layout.git#ee9c1a6ca765efce2d10b71a573cd8a8136196f9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^2.6.5",
    "echarts": "^4.2.1",
    "element-ui": "^2.11.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "svg-sprite-loader": "^4.1.6",
    "v-tooltip": "^2.0.2",
    "ve-charts": "^0.8.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.0.1"
  }
}
